            <property name="openjpa.ConnectionURL" value="jdbc:mysql://localhost:3306/stockmanagement"/>
            <property name="openjpa.ConnectionDriverName" value="com.mysql.cj.jdbc.Driver"/>
            <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/stockmanagement"/>
            <property name="eclipselink.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
